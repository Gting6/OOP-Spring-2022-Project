			layoutY="120.0" mnemonicParsing="false" prefHeight="39.0"
			prefWidth="179.0" text="Basic Info" />
		<Button fx:id="orderBtn" onAction="#pressOrderBtn"
			layoutX="45.0" layoutY="176.0" mnemonicParsing="false"
			prefHeight="39.0" prefWidth="179.0" text="Order" />
		<Label fx:id="displayLb" alignment="CENTER" layoutX="306.0"
			layoutY="88.0" prefHeight="236.0" prefWidth="274.0">
