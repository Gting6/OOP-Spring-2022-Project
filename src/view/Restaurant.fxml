	prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.RestaurantController">
	<children>
		<Label fx:id="welcomeLb" layoutX="45.0" layoutY="46.0"
			text="Restaurant Page">
			<font>
				<Font size="35.0" />
			</font>
		</Label>
		<Button fx:id="logoutBtn" onAction="#logout" layoutX="45.0"
			layoutY="338.0" mnemonicParsing="false" text="Log out" />
